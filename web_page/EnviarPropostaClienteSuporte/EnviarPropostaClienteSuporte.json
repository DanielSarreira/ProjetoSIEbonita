    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.proposta ){",
        "\treturn {",
        "\t\t//map proposta variable to expected task contract input",
        "\t\tpropostaInput: {",
        "\t\t\ttipoproposta: $data.proposta.tipoproposta !== undefined ? $data.proposta.tipoproposta : null,",
        "\t\t\tnecessidades: $data.proposta.necessidades.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tdescricao: it.descricao !== undefined ? it.descricao : null,",
        "\t\t\t\tesforco: it.esforco !== undefined ? it.esforco : null",
        "\t\t\t})),",
        "\t\t\tpreco: $data.proposta.preco !== undefined ? $data.proposta.preco : null,",
        "\t\t\tcondicoespagamento: $data.proposta.condicoespagamento !== undefined ? $data.proposta.condicoespagamento : null,",
        "\t\t\tis_aceite: $data.proposta.is_aceite !== undefined ? $data.proposta.is_aceite : null,",
        "\t\t\tis_standart: $data.proposta.is_standart !== undefined ? $data.proposta.is_standart : null,",
        "\t\t\tis_valida: $data.proposta.is_valida !== undefined ? $data.proposta.is_valida : null,",
        "\t\t\tis_suporte: $data.proposta.is_suporte !== undefined ? $data.proposta.is_suporte : null,",
        "\t\t\tcliente: $data.proposta.cliente ? {",
        "\t\t\t\tpersistenceId_string: $data.proposta.cliente.persistenceId_string !== undefined ? $data.proposta.cliente.persistenceId_string : null,",
        "\t\t\t\tnome: $data.proposta.cliente.nome !== undefined ? $data.proposta.cliente.nome : null,",
        "\t\t\t\tmorada: $data.proposta.cliente.morada !== undefined ? $data.proposta.cliente.morada : null",
        "\t\t\t} : null",
        "\t\t}",
        "\t}",
        "}"
